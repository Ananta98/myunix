+	__asm__ __volatile__ (
+		"int $0x80\n"
+		: "=a" (__ret)
+		: "a" ((long)n)
+	);
+	return __ret;
+}
+
+static inline long __syscall1(long n, long a1)
+{
+	unsigned long __ret;
+	__asm__ __volatile__ (
+		"int $0x80\n"
+		: "=a" (__ret)
+		: "a" (n), "b" (a1)
+	);
+	return __ret;
+}
+
+static inline long __syscall2(long n, long a1, long a2)
+{
+	unsigned long __ret;
+	__asm__ __volatile__ (
+		"int $0x80\n"
